<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" styleClass="root" stylesheets="@gui.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="practice.ControllerMain">
   <children>
      <StackPane fx:id="stackPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="302.0" prefWidth="507.0" spacing="10.0" StackPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" prefHeight="134.0" prefWidth="349.0" spacing="20.0">
                     <children>
                        <HBox prefHeight="48.0" prefWidth="367.0">
                           <children>
                              <Label id="status" fx:id="top" prefHeight="27.0" prefWidth="201.0" text="Login to proceed..." textAlignment="CENTER">
                                 <font>
                                    <Font name="Arial Black" size="18.0" />
                                 </font>
                              </Label>
                              <Label fx:id="status" prefHeight="27.0" prefWidth="177.0">
                                 <font>
                                    <Font name="Arial Black" size="14.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                        <TextField id="username" fx:id="username" prefHeight="26.0" prefWidth="271.0" promptText="Username" stylesheets="@gui.css">
                           <font>
                              <Font name="Arial" size="14.0" />
                           </font>
                           <VBox.margin>
                              <Insets right="75.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                  </VBox>
                  <HBox alignment="CENTER" prefHeight="66.0" prefWidth="427.0" spacing="10.0">
                     <children>
                        <PasswordField id="password" fx:id="password" prefHeight="26.0" prefWidth="276.0" promptText="Password">
                           <font>
                              <Font name="Arial" size="14.0" />
                           </font>
                        </PasswordField>
                        <Label fx:id="hint" prefHeight="26.0" prefWidth="65.0" text="hint">
                           <font>
                              <Font name="Arial Black" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="34.0" prefWidth="500.0" spacing="5.0">
                     <children>
                        <Button id="loginbtn" mnemonicParsing="false" onAction="#login" text="Log In">
                           <font>
                              <Font name="Arial Black" size="14.0" />
                           </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#hintpass" prefHeight="32.0" prefWidth="155.0" text="Forgot Password">
                           <font>
                              <Font name="Arial Black" size="14.0" />
                           </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#verifyCust" text="Customer Billing">
                           <font>
                              <Font name="Arial Black" size="14.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
               </children>
               <StackPane.margin>
                  <Insets left="50.0" />
               </StackPane.margin>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
